<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ButtonTransparentBorderlessTextBlueHeight17" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:backgroundTint">@color/transparent</item>
        <item name="android:minHeight">17dp</item>
        <item name="android:strokeWidth">0</item>
        <item name="android:padding">0dp</item>
        <item name="android:fontFamily">@font/sf_pro_display_semibold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="ButtonTransparentBorderlessHeight38" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:backgroundTint">@color/transparent</item>
        <item name="android:minHeight">38dp</item>
        <item name="android:strokeWidth">0</item>
        <item name="android:paddingHorizontal">0dp</item>
    </style>

    <style name="ButtonBlueBorderlessHeight48" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:backgroundTint">@color/blue</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:strokeWidth">0</item>
        <item name="android:fontFamily">@font/sf_pro_display_semibold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ButtonLightBlueBorderlessHeight29" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:backgroundTint">@color/blue_light</item>
        <item name="android:minHeight">29dp</item>
        <item name="android:strokeWidth">0</item>
        <item name="android:fontFamily">@font/sf_pro_display_semibold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:drawableRight">@drawable/arrow_right</item>
        <item name="android:drawableTint">@color/blue</item>
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="BookingTextInputLayout" parent="Widget.Design.TextInputLayout">
        <item name="android:textColorHint">@color/grey_hint</item>
    </style>

    <style name="BookingInputLayoutFilled" parent="Widget.Material3.TextInputLayout.FilledBox">
        <item name="android:textColorHint">@color/grey_hint</item>
    </style>
</resources>